{
  "name": "log1x/filament-starter",
  "type": "project",
  "description": "A beautiful starting point for your next TALL stack application.",
  "keywords": [
    "laravel",
    "livewire",
    "filament"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "abdulmajeed-jamaan/filament-translatable-tabs": "^3.0",
    "awcodes/filament-curator": "^3.3",
    "aymanalhattami/filament-slim-scrollbar": "^2.1",
    "bezhansalleh/filament-language-switch": "^3.1",
    "bezhansalleh/filament-shield": "*",
    "diogogpinto/filament-auth-ui-enhancer": "^1.0",
    "filament/filament": "^3.2.50",
    "hasnayeen/themes": "^3.0",
    "joaopaulolndev/filament-edit-profile": "^1.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.9",
    "livewire/livewire": "^3.3",
    "mcamara/laravel-localization": "^2.3",
    "njxqlus/filament-progressbar": "^1.1",
    "outerweb/filament-settings": "^1.3",
    "pboivin/filament-peek": "^2.2",
    "sinnbeck/markdom": "^2.0",
    "spatie/laravel-medialibrary": "^11.17",
    "spatie/laravel-translatable": "^6.11",
    "z3d0x/filament-logger": "^0.8.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9",
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },

    "files": [
      "app/Helpers/functions.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
